        System.out.print("Masukkan analisis kelembaban : ");
        int kb = (x.nextInt());
        System.out.print("Masukkan analisis suhu : ");
        int sh = (x.nextInt());
    }
    
    @Override
void analisis_identitasRK(){
    Scanner x = new Scanner(System.in);
        System.out.print("Masukkan Nama Ruang : ");
