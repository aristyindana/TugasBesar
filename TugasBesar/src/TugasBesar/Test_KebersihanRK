package TugasBesar;

public class Test_KebersihanRK {
KebersihanRK bersih = new KebersihanRK();
    
    String sirkulasiUdara (String sirkulasi_udara ){
    if ("lancar".equals(sirkulasi_udara)){
          return "sesuai";
    }
    else {
          return "tidak sesuai";
    }
    }
    
    String pencahayaan (int pencahayaan ){
    if (pencahayaan>=250 && pencahayaan <=350){
          return "sesuai";
    }
    else {
          return "tidak sesuai";
    }
    }
    
    String kelembapan (int kelembapan  ){
    if (kelembapan >=70 && kelembapan <=80){
          return "sesuai";
    }
    else {
          return "tidak sesuai";
    }
    }
           
    String suhu (int suhu ){
    if (suhu >=25 && suhu <=35){
          return "sesuai";
    }
    else {
          return "tidak sesuai";
    }
    }   
    
    }
